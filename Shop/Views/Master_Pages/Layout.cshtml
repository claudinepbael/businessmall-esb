<!DOCTYPE HTML>

<html class="no-js">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <!--[if lt IE 8]>
      <link rel="stylesheet" href="~/Content/blueprint/print.css" type="text/css" media="screen, projection">
    <![endif]-->
    <link href="~/Content/blueprint/print.css" rel="stylesheet" type="text/css" media="print" />
    <link href="~/Content/blueprint/screen.css" rel="stylesheet" type="text/css" media="screen, projection" />
    <link href="~/Content/Cisco/style.css" rel="stylesheet" />
    <link href="~/Content/Cisco/shop.css" rel="stylesheet" />
    <title>Cisco Businessmall</title>
    <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <script>window.jQuery || document.write('<script src="@Url.Content("~/Scripts/jquery-1.11.3.min.js")"><\/script>')</script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.11.4.js")" type="text/javascript"></script>
</head>

<body>
    <div class="container-account">
        <div class="header logon-header">
            <div class="main-header-inner-wrapper last">
                <div class="span-6 alpha logo first">
                    <img src="~/Content/Cisco/images/cisco_refresh.png" />
                </div>
                <div class="span-9 logo ">
                    <img class="outlet_logo" src="~/Content/Cisco/images/outlet_text.png" />
                </div>
                <div class="span-9 margin-left-35 margin-top-30 omega login-form last">
              
                </div>
            </div>
        </div>
        <div class="grid_12">
            <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Products", "ProductsList", "Products")</li>
                    <li>@Html.ActionLink("Orders", "History", "Orders")</li>
                </ul>
            </div>
        </nav>
        </div>
        <div class="grid_12 content-wrapper shop-body">
            @RenderBody()
        </div>

    </div>


</body>

</html>


